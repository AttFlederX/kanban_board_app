  "version": "0.2.0",
  "configurations": [
    {
      "name": "kanban_board_app",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "58072",
        "--dart-define-from-file",
        "${workspaceFolder}/local.env"
      ]
    },
    {
      "name": "kanban_board_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
